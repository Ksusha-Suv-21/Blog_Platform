    "antd": "^5.24.8",
    "axios": "^1.8.4",
    "date-fns": "^4.1.0",
    "markdown-to-jsx": "^7.7.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.1",
    "react-lazy": "^1.1.0",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router": "^7.5.0",
    "redux": "^5.0.1",
